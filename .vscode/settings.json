{
    "git.enabled": true,
    "breadcrumbs.enabled": false,
    "notebook.breadcrumbs.showCodeCells": false,
    "debug.toolBarLocation": "commandCenter",
    "editor.matchBrackets": "always",
    "editor.scrollbar.scrollByPage": false,
    "editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
    "editor.stickyScroll.enabled": false,
    "editor.formatOnSave": false,
    "breadcrumbs.filePath": "off",
    "outline.showFunctions": false,
    "breadcrumbs.showFunctions": false,
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome",
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "biomejs.biome",
    },
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome",
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "biomejs.biome",
    },
    "editor.renderLineHighlight": "gutter",
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.activeIconPack": "react",
    "material-icon-theme.folders.theme": "specific",
    "editor.lineHeight": 1.47,
    "explorer.compactFolders": false,
    "editor.minimap.enabled": false,
    "editor.fontLigatures": true,
    "editor.fontFamily": "JetBrains Mono, 'JetBrains Mono NL', Fira Code, 'Fira Code', JetBrains Mono, 'JetBrains Mono'",
    "workbench.tree.indent": 14,
    "window.zoomLevel": 0,
    "editor.fontSize": 14.5,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "workbench.sideBar.location": "left",
    "workbench.activityBar.location": "hidden",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "terminal.integrated.fontFamily": "JetBrains Mono, 'JetBrains Mono', Fira Code, 'Fira Code'",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1,
    "workbench.statusBar.visible": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": false,
    "workbench.tree.renderIndentGuides": "none",
    "editor.selectionHighlight": false,
    "workbench.editor.showTabs": "single",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "workbench.editor.editorActionsLocation": "hidden",
    "editor.lineNumbers": "on",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "explicit",
    "editor.cursorStyle": "underline",
    "editor.guides.highlightActiveIndentation": false,
    "editor.guides.indentation": false,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.autoClosingBrackets": "always",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "github.copilot.editor.enableAutoCompletions": true,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "**/Thumbs.db": false,
        "node_modules": false,
        ".vscode": false,
    },
    "emmet.excludeLanguages": [],
    "search.exclude": {
        "**/node_modules": false,
        "**/bower_components": false,
        "**/*.code-search": false
    },
    "explorer.excludeGitIgnore": false,
    "files.watcherExclude": {
        "**/.git/objects/**": false,
        "**/.git/subtree-cache/**": false,
        "**/.hg/store/**": false
    },
    "files.readonlyExclude": {},
    "explorer.autoRevealExclude": {
        "**/node_modules": false,
        "**/bower_components": false
    },
    "workbench.editor.centeredLayoutAutoResize": false,
    "workbench.editor.centeredLayoutFixedWidth": true,
    "[json]": {
        "editor.defaultFormatter": "biomejs.biome"
    }
}